{
  "name": "tyorka-admin",
  "scripts": {
    "start": "concurrently \"yarn workspace client start\" \"yarn workspace server start\"",
    "build": "yarn workspace server build && yarn workspace client build",
    "generate:json-schema": "typescript-json-schema ./packages/server/tsconfig.json ConfigIndex > config.schema.json"
  },
  "private": true,
  "packageManager": "yarn@3.1.0",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "concurrently": "^6.3.0",
    "typescript": "^4.4.4",
    "typescript-json-schema": "^0.53.0"
  }
}
